<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-sight-explorer-SightAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.gephi.sight.explorer.Bundle"/>
                <attr name="component" methodvalue="org.gephi.sight.explorer.SightTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.sight.explorer.Bundle#CTL_SightAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="SightAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-sight-explorer-SightAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="SightTopComponent.settings" url="SightTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="SightTopComponent.wstcref" url="SightTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
